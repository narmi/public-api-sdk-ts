{
  "name": "public-api-sdk-ts",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "nodemon dist/index.js",
    "download-schema": "curl -o narmi-public-api-schema.yaml https://online.sandbox.narmi.dev/v1/schema",
    "generate-sdk": "openapi-generator-cli generate -i narmi-public-api-schema.yaml -g typescript-fetch -o ./src/public-api-sdk --skip-validate-spec",
    "remove-sdk": "rm -rf ./src/public-api-sdk"
  },
  "keywords": [],
  "author": "Narmi",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "body-parser": "^1.20.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.1",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
